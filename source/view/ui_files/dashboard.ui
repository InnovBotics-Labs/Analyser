  <property name="styleSheet">
   <string notr="true">/* Main Window */
QMainWindow {
    background-color: #1B1B1B;
}

/* Sidebar (Icon Only Widget) */
#icon_only_widget {
    background-color: #252A34;
    border-radius: 10px;
}

/* Labels */
QLabel {
    font-size: 14px;
    color: #F5F5F5;
    font-weight: 500;
    transition: color 0.3s ease-in-out;
}
#Icon_label {
    color: #ECF0F1;
    font-size: 20px;
    font-weight: bold;
}

/* Buttons */
QPushButton {

    color: #ECF0F1;
    font-size: 14px;
    border: none;
    padding: 20px;
    width: 100%;
    transition: background-color 0.3s ease-in-out, transform 0.2s ease-in-out;
}
QPushButton:hover {
    background-color: #1A252F;
    transform: scale(1.05);
}
QPushButton:pressed {
    background-color: #16A085;
    transform: scale(0.98);
}
QPushButton:checked {
    background-color: #16A085;
    color: #FFFFFF;
    border: 2px solid #1ABC9C;
}

/* Specific Buttons */
#home_button_1, #earnings_button_1, #expenses_button_1, #settings_button_1, #logout_button_1, #mapper_button_1, #upload_button_1 , #change_button, #reload_button, #exit_button{
    text-align: left;
    padding-left: 12px;
    border-radius: 12px;
    padding: 10px;
}


#logout_button_1 {
    background-color: #D72638;
    transition: background-color 0.3s ease-in-out, transform 0.2s ease-in-out;
}
#logout_button_1:hover {
    background-color: #C0392B;
    transform: scale(1.05);
}

/* Full Name Widget */
#full_name_widget {
    background-color: #252A34;
    padding: 12px;
    border-radius: 10px;
    transition: background-color 0.3s ease-in-out;
}

/* ComboBox Styling */
QComboBox {
    background-color: #3A506B;
    color: #ECF0F1;
    font-size: 14px;
    border: 1px solid #1A252F;
    border-radius: 5px;
    padding: 5px;
}
QComboBox:hover {
    background-color: #1A252F;
}
QComboBox::drop-down {
    border: none;
    background-color: #34495E;
}
QComboBox QAbstractItemView {
    background-color: #2C3E50;
    color: #ECF0F1;
    border: 1px solid #1A252F;
    selection-background-color: #16A085;
}

/* Stacked Widget */
QStackedWidget {
    background-color: #1F1F1F;
    border-radius: 10px;
}

/* Table Widget */
QTableWidget {
    background-color: #2C3E50;
    color: #ECF0F1;
    gridline-color: #34495E;
    border: 1px solid #1A252F;
}
QHeaderView::section {
    background-color: #3A506B;
    color: #ECF0F1;
    padding: 5px;
    border: none;
}
QTableWidget::item:selected {
    background-color: #16A085;
    color: #FFFFFF;
}

/* Tree Widget */
QTreeWidget {
    background-color: #2C3E50;
    color: #ECF0F1;
    border: 1px solid #1A252F;
}
QTreeWidget::item:selected {
    background-color: #16A085;
    color: #FFFFFF;
}
QTreeView::branch:selected {
    background-color: #16A085;
}
